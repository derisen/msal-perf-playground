{
  "name": "msal-node-auth-code",
  "version": "1.0.0",
  "description": "sample web app for msal-node",
  "scripts": {
    "start": "node ./bin/www",
    "start:single": "npm start -- --im=single --cm=none --cs=1000 --mc=false --sn=auth-code-single-no-cache --out=./reports/benchmarks.md",
    "start:single-token-cache-session": "npm start -- --im=single --cm=session --cs=1000 --mc=false --sn=auth-code-single-token-cache-session --out=./reports/benchmarks.md",
    "start:multi": "npm start -- --im=multi --cm=none --cs=1000 --mc=false --sn=auth-code-multi-no-cache --out=./reports/benchmarks.md",
    "start:multi-token-cache-session": "npm start -- --im=multi --cm=session --cs=1000 --mc=false --sn=auth-code-multi-token-cache-session --out=./reports/benchmarks.md",
    "start:multi-token-metadata-cache-session": "npm start -- --im=multi --cm=session --cs=1000 --mc=true --sn=auth-code-multi-token-metadata-cache-session --out=./reports/benchmarks.md",
    "test": "playwright test",
    "postinstall": "npx playwright install --with-deps chromium"
  },
  "author": "Microsoft",
  "license": "MIT",
  "dependencies": {
    "@azure/msal-node": "^1.14.5",
    "axios": "^1.0.0",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "hbs": "^4.2.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.0",
    "autocannon": "^7.10.0",
    "clinic": "^12.0.0",
    "playwright": "^1.29.0"
  }
}
