{
  "name": "msal-node-auth-code",
  "version": "1.0.0",
  "description": "sample web app for msal-node",
  "scripts": {
    "start": "node ./bin/www",
    "start:auth-code-single-no-cache": "npm start -- --im=single --cm=none --cs=10000 --mc=false --sn=auth-code-single-no-cache --out=./reports/measurements.txt",
    "start:auth-code-single-token-cache-session": "npm start -- --im=single --cm=session --cs=10000 --mc=false --sn=auth-code-single-token-cache-session --out=./reports/measurements.txt",
    "start:auth-code-multi-token-cache-session": "npm start -- --im=multi --cm=session --cs=10000 --mc=false --sn=auth-code-multi-token-cache-session --out=./reports/measurements.txt",
    "start:auth-code-multi-token-metadata-cache-session": "npm start -- --im=multi --cm=session --cs=10000 --mc=true --sn=auth-code-multi-token-metadata-cache-session --out=./reports/measurements.txt",
    "start:auth-code-multi-token-metadata-cache-distributed": "npm start -- --im=multi --cm=distributed --cs=10000 --mc=true --sn=auth-code-multi-token-metadata-cache-distributed --out=./reports/measurements.txt",
    "test": "playwright test",
    "test:all": "cross-env TEST_SCENARIO=s1 playwright test && cross-env TEST_SCENARIO=s2 playwright test && cross-env TEST_SCENARIO=s3 playwright test && cross-env TEST_SCENARIO=s4 playwright test",
    "posttest": "node ../common/generateReport.js --t=webapp --m=../web-app/reports/measurements.txt --r=../web-app/reports/testruns.txt",
    "posttest:all": "node ../common/generateReport.js --t=webapp --m=../web-app/reports/measurements.txt --r=../web-app/reports/testruns.txt",
    "postinstall": "npx playwright install --with-deps chromium"
  },
  "author": "Microsoft",
  "license": "MIT",
  "dependencies": {
    "@azure/msal-node": "^1.14.5",
    "axios": "^1.0.0",
    "chalk": "^4.1.2",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "hbs": "^4.2.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.0",
    "clinic": "^12.0.0",
    "cross-env": "^7.0.3",
    "playwright": "^1.29.0"
  }
}
